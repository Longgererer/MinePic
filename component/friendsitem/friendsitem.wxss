.item{
  height: 100%;
  width: 100%;
  background-color: #fff;
  display: flex;
  align-items: center;
}
.img-view{
  width: 100rpx;
  height: 100rpx;
  border-radius: 60rpx;
  margin-left: 30rpx;
  overflow: hidden;
}
.img-view image{
  width: 100%;
  height: 100%;
}
.text{
  margin-left: 30rpx;
  font-size: 16px;
}
.hover{
  background-color: #e0dfdf !important;
}