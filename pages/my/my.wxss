.container{
    padding: 0;
}
.head-tab{
    display: flex;
    background-color: #f2f2f2;
    justify-content: center;
    align-items: center;
    width: 100%;
}
.head-tab-content{
    flex: 1;
    display: flex;
    align-items: center;
}
.items{
    width: 100%;
    margin: 20rpx 0;
}
.items-box:first-child{
    border-bottom: 1px solid #cecece;
    margin-bottom: 20rpx;
}
.items-box:last-child{
    border-bottom: 1px solid #cecece;
}
.hover{
    background-color: #e0dfdf !important;
}
my-head-tab{
    width: 100%;
}